{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn start",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "copyright": "Copyright Â© 2024 CodeDead",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Advanced PassGen is a free and open-source application that can help you generate passwords and export them",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "Generate passwords with ease",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "advanced-passgen",
  "mainBinaryName": "advanced-passgen",
  "version": "2.5.2",
  "identifier": "com.codedead.advancedpassgen",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 685,
        "resizable": true,
        "title": "Advanced PassGen",
        "width": 1000
      }
    ],
    "security": {
      "capabilities": ["main-capabilities"],
      "csp": null
    }
  }
